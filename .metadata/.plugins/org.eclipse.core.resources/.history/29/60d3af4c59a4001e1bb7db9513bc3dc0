package sub3;

/* 
*  날짜 : 2023/12/27
*  이름 : 정태영
*  내용 : Java 조건문 Switch 실습하기
*  
*/

public class ForTest {

	public static void main(String[] args) {
		
		// for
		for(int i=1; i<=5; i++) {
			System.out.println("i : " + i);
			
		}
		
		// 1부터 10까지 합
		int sum = 0;
		
		for(int k=1; k<=10; k++) {
			sum = ++k;
			
			
		}
		System.out.println("1부터 10까지 합 " + sum);
		// 1부터 10까지 짝수 합
		// for 중첩
		// 구구단
		// 별삼각형
	}
}
